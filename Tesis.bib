@article{Llop2008,
abstract = {This paper analyses the economic impact of alternative water policies implemented on the Spanish production system. The methodology comprises two versions of the input-output price model: an average cost formulation and a mark-up formulation. The input-output framework evaluates the impact of the various water policy measures on production prices, consumption prices, intermediate water use and private real income. The results show that a tax on the water used by sectors considerably reduces intermediate water consumption and increases production and consumption prices. On the other hand, according to Jevons' paradox, improvements in technical efficiency, which lead to a reduction in the water requirements of all sectors and an increase in water production, raise water consumption. The combination of a tax on the water used by sectors and an improvement in the technical efficiency of water in the production sphere takes the pressure off prices and significantly reduces intermediate water consumption. {\textcopyright} 2008 Elsevier B.V. All rights reserved.},
author = {Llop, Maria},
doi = {10.1016/j.ecolecon.2008.03.002},
file = {:Users/SebastianRiera/Library/Application Support/Mendeley Desktop/Downloaded/Llop - 2008 - Economic impact of alternative water policy scenarios in the Spanish production system An input-output analysis.pdf:pdf},
isbn = {0921-8009},
issn = {09218009},
journal = {Ecological Economics},
keywords = {Input-output analysis,Leontief price model,Water policy,Water taxation,Water use},
number = {1-2},
pages = {288--294},
title = {{Economic impact of alternative water policy scenarios in the Spanish production system: An input-output analysis}},
volume = {68},
year = {2008}
}
@article{Pulido-Velazquez2013,
abstract = {By ignoring the opportunity cost of water use, water is undervalued, which can lead to significant errors in investments and water allocation decisions. The marginal resource opportunity cost (MROC) varies in time and space, as resource availability, demands, and users' WTP vary. This spatial and temporal variability can only be captured by basinwide hydro-economic models integrating water demands and environmental requirements, resources, infrastructure, and operational and institutional restrictions. This paper presents a method for the simulation of water pricing policies linked to water availability, and the design of efficient pricing policies that incorporate the basinwide marginal value of water. Two approaches were applied: priority-based simulation and economic optimization. The improvement in economic efficiency was assessed by comparing the results from simulation of the current system operation and the pricing schedule. The difference between the benefits for the simulated current management and the upper bound benefits from optimization indicates the maximum gap that could be bridged with pricing. In the application to a synthetic case, a storage-dependent step pricing schedule derived from average MROC values led to benefits that capture 80{\%} of the gap of net benefits between management without pricing and the economically optimal man- agement. Different pricing policies were tested, depending not only on reservoir storage but also on previous inflows. The results show that the method is useful for designing pricing policies that enhance the economic benefits, leading to more efficient resource allocations over time and across the competing uses},
author = {Pulido-Vel{\'{a}}zquez, Manuel and Alvarez-Mendiola, E. and Andreu, J},
doi = {10.1061/(ASCE)WR.1943-5452.0000262},
file = {:Users/SebastianRiera/Google Drive/Biblio/Water Institutional Economics/Pulido-Velazques, M.$\backslash$; et al., 2013, Efficient water pricing policies w$\backslash$: opportunity costs.pdf:pdf},
issn = {0733-9496},
journal = {Journal of Water Resources Planning and Management},
month = {sep},
number = {5},
pages = {583--592},
title = {{Design of efficient water pricing policies integrating basinwide resource opportunity costs}},
volume = {139},
year = {2013}
}
