@inproceedings{Duek2013,
abstract = {Una importante concentraci{\'{o}}n de las ramas de actividad del sector industrial de la provincia de Mendoza pertenece a alimentos, vinos y bebidas, la que representa casi el 50{\%} del total de las empresas industriales. En zonas {\'{a}}ridas como Mendoza, el consumo y la eficiencia en el uso del agua requieren de especial atenci{\'{o}}n en todos los sectores de la demanda: dom{\'{e}}stico, riego, industrial y ambiental. Los establecimientos industriales de la provincia utilizan fundamentalmente agua subterr{\'{a}}nea, ya sea para refrigerar, como insumo para el producto o para la limpieza en distintas partes del proceso productivo; en este {\'{u}}ltimo caso, con retorno al sistema h{\'{i}}drico. El conocimiento del volumen de agua que utilizan estas industrias es importante para diversas aplicaciones, tales como el c{\'{a}}lculo del agua virtual de los productos, como tambi{\'{e}}n para la realizaci{\'{o}}n de balances h{\'{i}}dricos que incorporan los usos sectoriales a nivel de cuenca. Para conocer el consumo de agua por parte de la industria alimenticia de Mendoza, se propone utilizar coeficientes que relacionen volumen de agua con los insumos del proceso industrial, tales como materia prima procesada, mano de obra utilizada, producto elaborado. Estos coeficientes pueden ser aplicados para realizar las estimaciones a nivel de cuenca, del volumen de agua utilizada por estas industrias. El presente trabajo propone valores de coeficientes de uso industrial del agua para algunas ramas de actividad de las agroindustrias de Mendoza. Los coeficientes primarios se obtuvieron a partir de entrevistas presenciales en establecimientos industriales con diferentes escala de producci{\'{o}}n, tales como: bodegas, conserveras de frutas y hortalizas, cerveceras, bebidas no alcoh{\'{o}}licas y empacadoras de fruta. Los informantes fueron gerentes y/o t{\'{e}}cnicos de dichos establecimientos a quienes se les consult{\'{o}} sobre el agua consumida, la materia prima procesada y el producto elaborado. Los valores de coeficientes obtenidos a partir de las entrevistas se compararon con aquellos obtenidos en estudios anteriores y con la bibliograf{\'{i}}a consultada. Para cada rama de actividad se explica la utilizaci{\'{o}}n de agua en el proceso y se proponen rangos de valores para los coeficientes, en funci{\'{o}}n de: escala de producci{\'{o}}n, nivel de tecnolog{\'{i}}a empleada y conducta ambiental empresarial.},
address = {San Juan, Argentina},
author = {Duek, Alicia E and Fasciolo, Graciela E and Quiles, Mar{\'{i}}a Elena and Zoia, Oscar},
booktitle = {Congreso Nacional del Agua XXIV (CONAGUA)},
file = {:Users/SebastianRiera/Library/Application Support/Mendeley Desktop/Downloaded/Duek et al. - 2013 - Uso del agua en la industria alimenticia de Mendoza.pdf:pdf},
keywords = {coeficientes,industrias alimenticias,mendoza,s,usos del agua},
number = {1},
title = {{Uso del agua en la industria alimenticia de Mendoza}},
year = {2013}
}
@unpublished{Bravo-Uretaforthcoming,
abstract = {This paper extends previous work focusing on the analysis of technical efficiency (TE) for a sample of Chilean wine grape producers. We apply alternative panel data models to a cross-sectional dataset that contains multi-block observations for individual farms. The results show that when the individual blocks are treated as independent from each other, the average estimated TE scores are lower than those estimated when it is recognized that blocks belonging to a particular farm are subject to an overall central (farm-level) management and that individual blocks have a separate specific performance. This points to the importance of accounting for unobserved heterogeneity when repeated observations from individual farms are available on a cross-section. Regional location was also found to matter for grape production. Moreover, agro-climatic conditions were found to be influential on production levels where grape farms located on cooler zones produce significantly less than their counterparts in warmer zones. Overall, our estimations show the importance of accounting for both farm heterogeneity and agro-climatic conditions when analyzing TE in grape production.},
author = {Bravo-Ureta, Boris E. and Moreira, V{\'{i}}ctor H. and Troncoso, Javier L. and Wall, Alan and (forthcoming)},
file = {:Users/SebastianRiera/Google Drive/Biblio/Wine efficiency/2016. BBU.VM.JT{\&}AW.November 28.docx:docx},
keywords = {Chile,Cross Sectional Data,Fixed and Random Effects,Stochastic Frontiers,Technical Efficiency,Unobserved Heterogeneity,Wine Grapes},
title = {{Alternative Measures of Technical Efficiency and Unobserved Heterogeneity: Evidence from a Cross Section of Chilean Wine Grape Producers}},
year = {2017}
}
