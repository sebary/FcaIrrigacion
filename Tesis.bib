doi = {10.1080/00036846.2010.548789},
file = {:Users/SebastianRiera/Library/Application Support/Mendeley Desktop/Downloaded/Tamini, Larue, West - 2012 - Technical and environmental efficiencies and best management practices in agriculture.pdf:pdf},
isbn = {0003-6846$\backslash$r1466-4283},
