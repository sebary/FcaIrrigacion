@article{Castex2015,
abstract = {This paper explores current and future issues of water availability and use in one of the world's major wine-producing areas, Mendoza Province in Argentina. The region is located close to the Central Andes and is characterized by an arid to semi-arid climate, which exacerbates pressures on land-use and water resources for wine production. The principal supply of water originates in the snow and ice fields of the nearby Andean peaks. Already under stress today, the environmental, economic and societal structure of the region, highly dependent on the wine industry, is likely to be vulnerable to climate change in the course of the 21st century. In this study, the possible impacts of shifting temperature and precipitation patterns and climate variability linked to extreme events and El Ni{\~{n}}o/Southern Oscillation ENSO on water resources in the region and consequently on the wine industry will be investigated. While climatic factors already today exert a major control on water availability, water use is also shown to be influenced by non-climatic factors, in particular overexploitation of the resource and a certain degree of opacity of policies related to water rights and water allocation. Climate model results for the future suggest that there may be important changes in the quantity and timing of stream flow, highly dependent on the Andean cryosphere where many glaciers are already rapidly shrinking today. The direct impacts of a changing climate on wineries include extreme events and also decreased water availability for irrigation in the long term. In view of the importance wine as a major income-earner for Argentina, public water policies will need to address both the issues of unreliable current water governance practices and the adaptations necessary to cope with a changing climate.},
author = {Castex, Victorine and Tejeda, Enrique Mor{\'{a}}n and Beniston, Martin},
doi = {10.1016/j.envsci.2014.12.008},
file = {:Users/SebastianRiera/Library/Application Support/Mendeley Desktop/Downloaded/Castex, Tejeda, Beniston - 2015 - Water availability, use and governance in the wine producing region of Mendoza, Argentina.pdf:pdf},
isbn = {1462-9011},
issn = {14629011},
journal = {Environmental Science {\&} Policy},
keywords = {Climate variability,ENSO,Extreme events,Glaciers,Sustainability,Water availability,Water governance},
month = {apr},
pages = {1--8},
title = {{Water availability, use and governance in the wine producing region of Mendoza, Argentina}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S1462901114002366},
volume = {48},
year = {2015}
}
@book{OECD2012,
abstract = {The need to reform water policies is as urgent as ever. Water is$\backslash$nessential for economic growth, human health, and the environment. Yet governments around the world face significant challenges in$\backslash$nmanaging their water resources effectively. The problems are$\backslash$nmultiple and complex: billions of people are still without access$\backslash$nto safe water and adequate sanitation; competition for water is$\backslash$nincreasing among the different uses and users; and major$\backslash$ninvestment is required to maintain and improve water$\backslash$ninfrastructure in OECD and non-OECD countries. Despite progress$\backslash$non many fronts, governments around the world are still confronted$\backslash$nwith the need to reform their existing water policies in order to$\backslash$nmeet current objectives and future challenges. Building on the$\backslash$nwater challenges identified by the OECD Environment Outlook to$\backslash$n2050, this report examines three fundamental areas that need to$\backslash$nbe addressed whatever reform agendas are pursued by governments:$\backslash$nfinancing of the water sector; the governance and institutional$\backslash$narrangements that are in place; and coherence between water$\backslash$npolicies and policies in place in other sectors of the economy.$\backslash$nThe report provides governments with practical advice and policy$\backslash$ntools to pursue urgent reform in their water sectors.},
