@article{Bertranou1983,
abstract = {The present paper briefly exposes several aspects related to water use, emphasizing the role of water discharge and re-use in agriculture. Mendoza is an Argentinian State located in a typical arid region. AS such, Mendoza has grown on the basis of intensive surface and ground water use, confronting today a clear overexploitation of the ground water reserves. This situation imposes a burden of increasing pumping costs and decreasing water quality for future generations. As it is well known, the common use of a natural resource (ground water reserves) is characterized by a time externality, which constitutes a market imperfection. In addition to this population growth, improvement in urban water provision and sewage services, increasing industrial water demand, etc., impose a host of problems in terms of water quantity and quality. In all these cases, it is commonly agreed, public action is required. It is our claim that most of the existing problems in relation to water use in Mendoza are common to most of the arid regions of the world. Hence, the present should constitute, at least partially, an adequate starting point for the establishment of rational guidelines in water management in arid zones. The second section of this paper contains a description of the nature of the regional water sources, centering attention on the water cycle in relation to agricultural use and its impact upon water quantity and quality. It also contains a brief description of the flood prevention system. The third section shows a characterization of the urban water provision and sewage system in the region, noticing the existing interrelationship between urban sewage and the irrigation system. A quantification of the volumes of water involved is provided. In the fourth section a similar description is made with regard to the industrial sector, where the amount of water used and an estimate of the industrial effluents organic load is advanced. A listing of social and economic impacts associated to water use and re-use in the region is presented in the fifth section, where the role and deficiencies of the present water management structure are set forth. Finally, the sixth section contains a set of recommendations aiming towards the establishment of a decision integrated management system.},
author = {Bertranou, Armando V. and Llop, Armando and Vazquez, Arturo J.},
doi = {10.1080/02508068308685991},
file = {:Users/SebastianRiera/Library/Application Support/Mendeley Desktop/Downloaded/Bertranou, Llop, Vazquez - 1983 - Water Use and Reuse Management in Arid Zones—The Case of Mendoza, Argentina.pdf:pdf},
issn = {0250-8060},
journal = {Water International},
month = {jan},
number = {1},
pages = {2--12},
title = {{Water Use and Reuse Management in Arid Zones—The Case of Mendoza, Argentina}},
url = {http://www.tandfonline.com/doi/abs/10.1080/02508068308685991},
volume = {8},
year = {1983}
}
@inproceedings{Gonzalez2011,
abstract = {Lobesia botrana ,es una de las principales plagas de los vi{\~{n}}edos en Europa desde el 1800. Fue descrita por Denis {\&} Schiffm{\"{u}}ller en Austria en vid .Desde ese momento comienza a extenderse por la zona del Mediterr{\'{a}}neo. En Am{\'{e}}rica es declarada por Chile en abril de 2008, en octubre de 2009 por California. En Mendoza, en febrero de 2010, se evidencia su presencia. Es plaga cuarentenaria para Argentina. Se realizaron estudios de bioecolog{\'{i}}a y comportamiento en las condiciones agroecol{\'{o}}gicas de Mendoza como base para el logro de programas de manejo racionales de la plaga. Para la obtenci{\'{o}}n de la din{\'{a}}mica poblacional, se realiz{\'{o}} en forma semanal muestreos en vi{\~{n}}edos infestados de Lujan de Cuyo y Maip{\'{u}}. Para determinar el inicio de vuelos de machos de Lobesia botrana se colocaron trampas de feromonas. En la primera generaci{\'{o}}n, se monitorearon 100 inflores- cencias por semana. En las siguientes se hizo un muestreo dirigido, tomando s{\'{o}}lo racimos infestados. Las muestras, se llevaron a laboratorio donde bajo lupa binocular se determin{\'{o}} el estado que presentaba la plaga en ese momento. Las trampas se monitorearon dos veces por semana. Pasa el invierno como pupa protegida por un capullo bajo la ritidomis en la base del pit{\'{o}}n, brazos y parte superior del tronco. A inicios de setiembre se observaron los primeros vuelos de adultos. Las hembras colocaron huevos en forma aislada sobre las br{\'{a}}cteas florales de la inflorescencia, la corola y el pedicelo. Las larvas neonatas se alimentaron de los botones florales, introduci{\'{e}}ndose y consumiendo los estambres y el ovario. Mediante hilos sedosos unen los restos de flores formando los “glom{\'{e}}rulos”. Esta etapa dur{\'{o}} aproximadamente un mes, tras el cual se trasforma en cris{\'{a}}lida. El empupamiento lo realiz{\'{o}} dentro de los “glom{\'{e}}rulos” en la inflorescencia, en hojas y bajo la corteza. A fines de noviembre se inicia el segundo vuelo. La puesta de la segunda generaci{\'{o}}n tiene lugar sobre las bayas, generalmente una por grano. Las larvas penetran y producen el vaciamiento de los mismos, inclusive de las semillas cuando {\'{e}}stas todav{\'{i}}a no se han endurecido. Empupan dentro de bayas deshidratadas y en hojas. A partir de la segunda quincena de enero comenz{\'{o}} el tercer vuelo, extendi{\'{e}}ndose un mes. La postura fue realizada sobre granos en envero, con las mismas caracter{\'{i}}sticas de la segunda generaci{\'{o}}n. Un gran porcentaje de las larvas de esta generaci{\'{o}}n entr{\'{o}} en diapausa, empupando bajo la corteza. Palabras claves: bioecolog{\'{i}}a, Lobesia botrana, trampas de feromonas, racimos, vid.},
address = {Mendoza, Argentina},
author = {Gonz{\'{a}}lez, Marcela F.},
booktitle = {1er Simposio Argentino de Viticultura y Enolog{\'{i}}a},
title = {{Estudio de la Bioecolog{\'{i}}a de Lobesia Botrana Den. {\&} Shiff en las Condiciones Agroecol{\'{o}}gicas del Oasis Norte de Mendoza}},
year = {2011}
}
