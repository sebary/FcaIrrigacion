journal = {Agricultural Sciences in China},
keywords = {Tobit regression,data envelopment analysis,determinants,irrigation water efficiency,tobit regression},
month = {sep},
number = {9},
pages = {1326--1337},
publisher = {Chinese Academy of Agricultural Sciences},
title = {{Irrigation Water Use Efficiency of Farmers and Its Determinants: Evidence from a Survey in Northwestern China}},
