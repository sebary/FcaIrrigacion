file = {:Users/SebastianRiera/Google Drive/Biblio/Efficiency/F{\"{a}}re, R., Grosskopf, S., Hernandez-Sancho, 2004, Environmental Performance, an index approach.pdf:pdf},
isbn = {0928-7655},
issn = {09287655},
journal = {Resource and Energy Economics},
keywords = {Environmental performance,Malmquist indexes},
number = {4},
pages = {343--352},
title = {{Environmental performance: An index number approach}},
volume = {26},
